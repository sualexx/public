{
  "name": "@sualexx/next-app",
  "private": true,
  "scripts": {
    "build": "next build",
    "commit": "git-cz",
    "dev": "cross-env NODE_OPTIONS='--inspect' next",
    "seed": "babel-node ./src/db/seed.js",
    "start": "next start"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-ssr": "^3.1.5",
