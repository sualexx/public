    "apollo-server-micro": "^2.14.2",
    "clsx": "^1.1.0",
    "dotenv": "^8.2.0",
    "graphql-scalars": "^1.1.2",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^5.0.0",
    "mongoose": "^5.9.13",
    "next": "9.3.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "commitizen": "^4.1.2",
    "conventional-github-releaser": "^3.1.3",
